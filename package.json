{
  "name": "deliver_it",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch-backend": "nodemon",
    "start": "concurrently -n frontend,backend -c blue,red 'npm run frontend' 'npm run watch-backend'",
    "frontend": "webpack-dev-server",
    "backend": "./node_modules/.bin/ts-node --project tsconfig.server.json --files src/server/main.ts",
    "postbuild": "npm run modules && npm run f-dockerfiles && npm run b-dockerfiles",
    "modules": "cp -R package.json dist/server/package.json",
    "f-dockerfiles": "cp src/server/Dockerfile dist/server/Dockerfile",
    "b-dockerfiles": "cp src/client/Dockerfile build/Dockerfile",
    "build": "concurrently -n frontend,backend -c blue,red 'npm run frontend-build' 'npm run backend-build'",
    "backend-build": "./node_modules/.bin/tsc --project tsconfig.server.json",
    "frontend-build": "NODE_ENV=production webpack --mode production",
    "typings": "concurrently -n sql,graphql -c green,white 'npm run sql-typings' 'npm run graphql-typings'",
    "sql-typings": "pgtyped -c config.json",
    "graphql-typings": "graphql-codegen --config codegen.yml"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@date-io/date-fns": "1.3.13",
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "@material-ui/pickers": "3.2.10",
    "@types/cors": "2.8.8",
    "@types/react": "16.9.51",
    "@types/react-dom": "16.9.8",
    "body-parser": "1.19.0",
    "cors": "2.8.5",
    "formik-material-ui": "3.0.0",
    "react": "16.13.1",
    "react-currency-format": "1.0.0",
    "react-dom": "16.13.1",
    "react-number-format": "4.4.1"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-transform-runtime": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@babel/runtime": "7.11.2",
    "@pgtyped/query": "0.8.2",
    "@pgtyped/cli": "0.8.2",
    "@typescript-eslint/eslint-plugin": "4.4.0",
    "@typescript-eslint/parser": "4.4.0",
    "awesome-typescript-loader": "5.2.1",
    "babel-loader": "8.1.0",
    "concurrently": "5.3.0",
    "css-loader": "4.3.0",
    "eslint": "7.11.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.1.4",
    "eslint-plugin-react": "7.21.4",
    "eslint-plugin-react-hooks": "4.1.2",
    "fork-ts-checker-webpack-plugin": "5.2.0",
    "formik": "2.2.1",
    "html-webpack-plugin": "4.5.0",
    "mini-css-extract-plugin": "0.12.0",
    "nodemon": "2.0.4",
    "pg": "8.4.2",
    "prettier": "2.1.2",
    "source-map-loader": "1.1.0",
    "ts-loader": "8.0.4",
    "ts-node": "9.0.0",
